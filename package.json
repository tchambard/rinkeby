{
  "name": "rinkeby",
  "version": "1.0.0",
  "scripts": {
    "start-ganache": "ganache -h localhost -m \"chef case useless snow uphold ignore biology devote fit fancy below allow\"",
    "lint:all": "yarn workspaces foreach -p run lint",
    "clean:all": "yarn workspaces foreach -p run clean",
    "compile:all": "yarn workspaces foreach -pt run compile",
    "test:all": "yarn workspaces foreach -t -j 1 run test",
    "g:clean": "cd $INIT_CWD && rimraf build",
    "g:tslint": "cd $INIT_CWD && tslint --format verbose \"src/**/*.ts\"",
    "g:tslint-fix": "cd $INIT_CWD && yarn g:tslint -- --fix",
    "g:compile": "cd $INIT_CWD && echo $INIT_CWD && tsc -v && tsc --pretty",
    "g:compile-watch": "cd $INIT_CWD && tsc --pretty -w",
    "g:clean-compile": "cd $INIT_CWD && yarn g:clean && yarn g:compile",
    "g:clean-lint-compile": "cd $INIT_CWD && yarn g:clean && yarn g:tslint && yarn g:compile",
    "g:depcheck": "cd $INIT_CWD && depcheck",
    "g:test": "cd $INIT_CWD && CONF_FILE=$INIT_CWD/build/config/config_test.js mocha --full-trace --recursive --exit --require source-map-support/register --ui f-mocha 'build/test/**/*.spec.js'",
    "g:test-watch": "cd $INIT_CWD && CONF_FILE=$INIT_CWD/build/config/config_test.js mocha -w -R spec --full-trace --recursive --require source-map-support/register --ui f-mocha 'build/test/**/*.spec.js'",
    "g:coverage": "cd $INIT_CWD && CONF_FILE=$INIT_CWD/build/config/config_test.js nyc mocha --full-trace --recursive --require source-map-support/register --ui f-mocha 'build/test/**/*.spec.js'",
    "g:solhint": "cd $INIT_CWD && solhint -q contracts/**/*.sol",
    "g:truffle-compile": "cd $INIT_CWD && truffle compile",
    "g:truffle-test": "cd $INIT_CWD && truffle test",
    "g:truffle-migrate": "cd $INIT_CWD && truffle migrate",
    "g:truffle-deploy": "cd $INIT_CWD && truffle deploy",
    "g:typechain-truffle": "cd $INIT_CWD && typechain --target=truffle-v5 --out-dir types/truffle 'build/contracts/*.json'",
    "g:typechain-web3": "cd $INIT_CWD && typechain --target=web3-v1 --out-dir types/web3 'build/contracts/*.json'"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.16",
    "@truffle/hdwallet-provider": "^2.1.0",
    "@typechain/truffle-v5": "^8.0.1",
    "@typechain/web3-v1": "^6.0.1",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^18.8.4",
    "@yarnpkg/cli": "^2.4.0",
    "@yarnpkg/core": "^2.4.0",
    "chai": "^4.3.6",
    "ganache": "^7.4.4",
    "mocha": "^10.1.0",
    "npm": "^7.6.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "solhint": "^3.3.7",
    "truffle": "^5.6.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4",
    "yargs": "^16.2.0"
  },
  "resolutions": {
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.14"
  },
  "author": "teddy.chambard@gmail.com",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@3.1.1"
}
