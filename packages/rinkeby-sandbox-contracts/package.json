{
  "name": "rinkeby-sandbox-contracts",
  "version": "1.0.0",
  "types": "index.d.ts",
  "scripts": {
    "compile": "truffle compile && yarn run generate-types-truffle && yarn run generate-types-web3",
    "generate-types-truffle": "typechain --target=truffle-v5 --out-dir types/truffle 'build/contracts/*.json'",
    "generate-types-web3": "typechain --target=web3-v1 --out-dir types/web3 'build/contracts/*.json'",
    "test": "truffle test",
    "migrate": "truffle migrate",
    "deploy": "truffle deploy"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@typechain/truffle-v5": "^8.0.1",
    "chai": "^4.3.0",
    "typechain": "^8.1.0"
  },
  "author": "teddy.chambard@gmail.com",
  "license": "MIT"
}
